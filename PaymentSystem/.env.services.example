# Environment za Services Stack (PSP, Bank, PayPal)
# Koristiti: docker compose -f docker-compose.services.yml --env-file .env.services up -d

# PostgreSQL
POSTGRES_PSP_PASSWORD=psp_password_2024
POSTGRES_BANK_PASSWORD=bank_password_2024
POSTGRES_PAYPAL_PASSWORD=paypal_password_2024

# JWT Secret
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production-32chars-min

# Bank
BANK_MERCHANT_ID=WEBSHOP_001
BANK_HMAC_SECRET=bank-hmac-secret-key-change-in-production

# PayPal
PAYPAL_CLIENT_ID=your_paypal_client_id
